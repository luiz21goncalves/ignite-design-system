{
  "extends": ["@rocketseat/eslint-config/react"],
  "ignorePatterns": ["node_modules", "dist"],
  "rules": {
    "import/order": [
      "warn",
      {
        "groups": [
          ["builtin", "external"], "internal", ["parent", "sibling", "index"]
        ],
        "newlines-between": "always-and-inside-groups"
      }
    ]
  }
}
